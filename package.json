{
  "name": "slidev-theme-divriots",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "slidev": {
    "colorSchema": "both",
    "highlighter": "shiki",
    "defaults": {
      "routerMode": "hash",
      "colorSchema": "dark",
      "highlighter": "shiki",
      "fonts": {
        "sans": "Space Grotesk",
        "mono": "Fira Code"
      }
    }
  },
  "scripts": {
    "dev": "slidev slides.md",
    "build": "slidev build slides.md",
    "export": "slidev export slides.md",
    "screenshot": "slidev export slides.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.28.6",
    "codemirror-theme-vars": "^0.1.1",
    "date-fns": "^2.28.0",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-smartarrows": "^1.0.1",
    "prism-theme-vars": "^0.2.2",
    "qrcode.vue": "^3.3.3",
    "theme-vitesse": "^0.1.14"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.0.12",
    "@iconify-json/codicon": "^1.0.13",
    "@iconify-json/ic": "^1.1.1",
    "@iconify-json/mdi-light": "^1.1.0",
    "@slidev/cli": "^0.28.6",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^7.20.0"
  }
}
